    set encoding utf8
    set term pngcairo size 1532,600 enhanced font 'Verdana'
    set grid front
    set xlabel "min"
    set y2range [0 : 40]
    set y2tics
    set yrange [-40: 0]
    set key bottom right
    set ylabel "m" rotate by 0
    set y2label "Â°C" rotate by 0
    set samples 150
